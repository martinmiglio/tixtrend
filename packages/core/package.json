{
  "name": "@tixtrend/core",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./modules/*": "./src/modules/*/index.ts",
    "./lib/*": "./src/lib/*/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "prettier -u -l . && eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "prettier --write . && eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.911.0",
    "@aws-sdk/client-sqs": "^3.911.0",
    "@aws-sdk/lib-dynamodb": "^3.911.0",
    "valibot": "^1.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tixtrend/eslint-config": "workspace:*",
    "@tixtrend/prettier-config": "workspace:*",
    "@types/node": "^24.7.2",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-vitest": "^6.2.1",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  }
}
